<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>com.indexoutofbounds.launch.mount_disks</string>

		<key>UserName</key>
		<string>me</string>

		<key>Program</key>
		<string>/Users/me/bin/mount_disks.sh</string>

		<key>WorkingDirectory</key>
		<string>/Users/me</string>

		<key>StandardErrorPath</key>
		<string>/Users/me/log/mount_disk/err.log</string>

		<key>StandardOutPath</key>
		<string>/Users/me/log/mount_disk/std.log</string>

		<key>StartInterval</key>
		<integer>60</integer>

		<key>EnvironmentVariables</key>
		<dict>
			<key>NASPASSWORD</key>
			<string>password</string>
			<key>NASUSERID</key>
			<string>user</string>
		</dict>
	</dict>
</plist>